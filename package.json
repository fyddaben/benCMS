    "egg-passport-local": "^1.2.1",
    "egg-scripts": "^2.6.0",
    "egg-view-ejs": "^2.0.0",
    "element-ui": "^2.4.5",
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
